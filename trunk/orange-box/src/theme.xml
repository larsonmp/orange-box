<?xml version="1.0" encoding="UTF-8" ?>
<ConfigMaps>
  <ConfigMap type="Skin">
    <Meta name="title">Lambda-Crowbar</Meta>
    <Meta name="description">An homage to crowbars, and the 11th letter in the Greek alphabet.</Meta>
    <Meta name="author">Micah Larson</Meta>
    <Meta name="author_location">Colorado, USA</Meta>
    <Meta name="author_email">larson.micah@gmail.com</Meta>
    <!--
    <Meta name="thumbnail"></Meta>
    <Meta name="screenshot"></Meta>
    -->
  </ConfigMap>
  <ConfigMap type="Skin">
    <Attribute name="header.background_color">#c3c3c3</Attribute>
    <Attribute name="header.center_image.url">http://orange-box.googlecode.com/svn/trunk/orange-box/src/resources/header_image.png</Attribute>
    <Attribute name="header.logo">beveled_white</Attribute>
    <Attribute name="header.link_color">#fb7e14</Attribute>
    <Attribute name="header.text_color">#fb7e14</Attribute>
    <Attribute name="navbar.background_color">#c3c3c3</Attribute>
    <Attribute name="navbar.border_color">#fb7e14</Attribute>
    <!-- #fb7e14 is not a supported icon color (yet) -->
    <Attribute name="navbar.icons.icon_color">#000000</Attribute>
    <Attribute name="navbar.tab.selected.link_color">#fb7e14</Attribute>
    <Attribute name="navbar.tab.unselected.link_color">black</Attribute>
    <Attribute name="navbar.tab.unselected.border_color">black</Attribute>
    <Attribute name="navbar.contacts.contact_color">black</Attribute>
    <Attribute name="navbar.contacts.status_color">#fb7e14</Attribute>
    <Attribute name="navbar.contacts.hover_background_color">#c3c3c3</Attribute>
    <Attribute name="navbar.contacts.card.border_color">#fb7e14</Attribute>
    <Attribute name="navbar.contacts.invite.background_color">white</Attribute>
    <Attribute name="navbar.contacts.invite.border_color">#fb7e14</Attribute>
    <Attribute name="navbar.contacts.invite.text_color">#fb7e14</Attribute>
    <Attribute name="navbar.contacts.invite.link_color">#fb7e14</Attribute>
    <Attribute name="gadget_area.chat.header.background_color">black</Attribute>
    <Attribute name="gadget_area.chat.header.highlight.background_color">white</Attribute>

    <!-- DEPEND ON USER'S VERSION OF iGoogle
    <Attribute name="gadget_area.tab.border_color">#fb7e14</Attribute>
    <Attribute name="gadget_area.tab.selected.background_color ">black</Attribute>
    <Attribute name="gadget_area.tab.selected.text_color ">white</Attribute>
    <Attribute name="gadget_area.tab.unselected.background_color">#c3c3c3</Attribute>
    <Attribute name="gadget_area.tab.unselected.text_color ">black</Attribute>
    END VERSION-DEPENDANT SECTION-->

    <Attribute name="gadget_area.border_color">#fb7e14</Attribute>
    <Attribute name="gadget_area.gadget.border_color">#c3c3c3</Attribute>
    <Attribute name="gadget_area.gadget.hover.border_color">#fb7e14</Attribute>
    <Attribute name="gadget_area.gadget.header.background_color">black</Attribute>
    <Attribute name="gadget_area.gadget.header.text_color">white</Attribute>
    <Attribute name="gadget_area.gadget.body.link_color">#fb7e14</Attribute>
    <!-- #fb7e14 is not a supported icon color (yet) -->
    <Attribute name="gadget_area.icons.icon_color">#ffffff</Attribute>
    <Attribute name="footer.background_color">#c3c3c3</Attribute>
    <Attribute name="footer.center_image.url">http://orange-box.googlecode.com/svn/trunk/orange-box/src/resources/footer_image.png</Attribute>
    <Attribute name="footer.link_color">#fb7e14</Attribute>
    <Attribute name="footer.text_color">black</Attribute>
    
    <!-- UNUSED ATTRIBUTES
    <Attribute name="header.tile_image.url"></Attribute>
    <Attribute name="footer.tile_image.url"></Attribute>
    <Attribute name="attribution.image.url"></Attribute>
    -->
  </ConfigMap>
</ConfigMaps>
